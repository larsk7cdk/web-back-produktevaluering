@model IEnumerable<web_back_produktevaluering.web.Models.Evaluering>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Navn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Produkt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Karakter)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Navn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Produkt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Karakter)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EvalueringId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EvalueringId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EvalueringId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
